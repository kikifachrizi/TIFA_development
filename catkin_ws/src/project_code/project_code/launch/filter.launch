<launch>
  <!-- Node Filter -->
  <node pkg="laser_filters" type="scan_to_scan_filter_chain" name="scan_filter_node" output="screen">
    <!-- Gunakan file YAML untuk konfigurasi parameter -->
    <rosparam command="load" file="/home/tsabit/catkin_ws/src/project_code/config/scan_filter.yaml" />
    
    <!-- Remapping topik input dan output -->
    <remap from="scan" to="/scan"/> <!-- Topik input -->
