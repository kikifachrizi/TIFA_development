<launch>

    <!-- Move Base Node -->
    <node pkg="move_base" type="move_base" name="move_base" output="screen">
      <param name="base_global_planner" value="navfn/NavfnROS"/>
      <param name="global_costmap/inflation_layer/inflation_radius" value="0.3"/>
      <param name="local_costmap/inflation_layer/inflation_radius" value="0.3"/>
      
      <rosparam file="$(find project_code)/config/costmap_params.yaml" command="load" />
      
    </node>
  
  </launch>
  
